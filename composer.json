{
    "name": "wedevelopnl/silverstripe-staging-warning",
    "description": "A module that adds some warning to the staging admin.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "authors": [
        {
            "name": "Dennis Prins",
            "email": "dennis@wedevelop.nl"
        }
    ],
    "require": {
        "php": "^8.1",
        "silverstripe/framework": "^5",
        "silverstripe/admin": "^2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.20",
        "syntro/silverstripe-phpstan": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "WeDevelop\\StagingWarning\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": false,
            "silverstripe/vendor-plugin": false
        }
    },
    "extra": {
        "installer-name": "silverstripe-staging-warning",
        "expose": [
            "client/dist"
        ]
    }
}
